{
	"name": "express-typescript-starter",
	"version": "0.1.0",
	"description": "A starting point for Node.js express apps with TypeScript",
	"repository": {
		"type": "git",
		"url": "https://github.com/sahat/hackathon-starter.git"
	},
	"author": "TMT HITEK",
	"license": "MIT",
	"scripts": {
		"start": "npm run serve:gcloud",
		"sequelize": "./node_modules/.bin/sequelize",
		"sequelize:migrate": "./node_modules/.bin/sequelize db:migrate",
		"deploy:gcloud": "npm run build && rm -rf ./dist && gcloud app deploy",
		"serve:gcloud": "npm run build-babel && node dist/index.js",
		"build": "npm run clear && npm run build-ts && npm run build-babel",
		"serve": "nodemon dist/server.js",
		"watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-babel\" \"npm run serve\"",
		"test": "jest --forceExit",
		"build-ts": "tsc",
		"watch-ts": "tsc -w",
		"build-webpack": "webpack",
		"watch-webpack": "webpack --watch",
		"build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
		"watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
		"tslint": "tslint -c tslint.json -p tsconfig.json",
		"tslint-fix": "tslint -c tslint.json -p tsconfig.json --fix",
		"copy-static-assets": "node copyStaticAssets.js",
		"debug": "npm run build && npm run watch-debug",
		"serve-debug": "nodemon --inspect dist/server.js",
		"watch-debug": "npm run clear && concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-babel\"",
		"dev": "npm run build && nodemon dist/index.js",
		"build-babel": "./node_modules/.bin/babel build --out-dir dist --source-maps",
		"watch-babel": "./node_modules/.bin/babel-watch build/index.js",
		"watch-babel:prod": "NODE_ENV=production ./node_modules/.bin/babel-watch build/index.js",
		"watch-babel:prod:win": ".\\node_modules\\.bin\\babel-watch build\\index.js",
		"clear": "rm -rf ./build && rm -rf ./dist",
		"start:dev": "concurrently -k -p \"[{name}]\" -n \"WATCH,HTTP,OTHER\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-babel\" \"npm run watch-ts\"",
		"docker:mysql": "cd docker && docker-compose -f docker-compose.mysql.yml up -d",
		"docker:mysql:stop": "cd docker && docker-compose -f docker-compose.mysql.yml stop",
		"migrate": "sequelize db:migrate --env development",
		"prepare": "chmod +x ./node_modules/husky/lib/bin.js && husky install",
		"lint": "eslint --ignore-path .eslintignore \"src/**/*.ts\" --fix",
		"lint:check": "eslint --ignore-path .eslintignore \"src/**/*.ts\"",
		"db:migrate": "npx sequelize-cli db:migrate --name",
		"db:migration": "npx sequelize migration:create --name",
		"db:migrate:undo": "npx sequelize db:migrate:undo --name",
		"format": "prettier --write ."
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown",
		"*.ts": [
			"npm run lint"
		]
	},
	"jest": {
		"globals": {
			"__TS_CONFIG__": "tsconfig.json"
		},
		"moduleFileExtensions": [
			"ts",
			"js"
		],
		"transform": {
			"^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
		},
		"testMatch": [
			"**/test/**/*.test.(ts|js)"
		],
		"testEnvironment": "node",
		"moduleNameMapper": {
			"@/(.*)": "./src/$1"
		},
		"moduleDirectories": [
			"src"
		]
	},
	"dependencies": {
		"@commitlint/config-conventional": "^17.7.0",
		"@faker-js/faker": "^9.0.3",
		"@types/bcrypt": "^5.0.0",
		"@types/sequelize": "^4.28.14",
		"@typescript-eslint/eslint-plugin": "^5.58.0",
		"@typescript-eslint/parser": "^5.58.0",
		"ajv": "^5.2.3",
		"ajv-errors": "^1.0.0",
		"ajv-keywords": "^2.1.0",
		"axios": "^0.21.1",
		"bcrypt": "^5.0.1",
		"body-parser": "^1.18.2",
		"buffer-image-size": "^0.6.4",
		"cors": "^2.8.4",
		"crypto": "^1.0.1",
		"dotenv": "^4.0.0",
		"express": "^4.16.3",
		"express-session": "^1.15.6",
		"express-validator": "^4.2.1",
		"fetch": "^1.1.0",
		"firebase-admin": "^5.13.1",
		"jwt-simple": "^0.5.1",
		"lodash": "^4.17.4",
		"md5": "^2.3.0",
		"moment": "^2.18.1",
		"multer": "^1.3.0",
		"mysql2": "^3.11.3",
		"node-cron": "^3.0.2",
		"node-fetch": "^3.3.0",
		"node-schedule": "^1.3.0",
		"npm": "^6.9.0",
		"querystring": "^0.2.1",
		"redis": "^4.6.7",
		"sequelize": "^6.29.0",
		"socket.io": "^2.1.1",
		"stream": "0.0.2",
		"stripe": "^12.5.0",
		"typescript": "^5.0.4",
		"uuidv4": "^6.2.13"
	},
	"devDependencies": {
		"@babel/core": "^7.18.9",
		"@babel/preset-env": "^7.18.9",
		"@commitlint/cli": "^17.5.1",
		"@types/async": "^2.0.40",
		"@types/bluebird": "^3.5.16",
		"@types/body-parser": "^1.16.2",
		"@types/bson": "^4.0.5",
		"@types/compression": "0.0.33",
		"@types/cors": "^2.8.1",
		"@types/del": "^3.0.1",
		"@types/dotenv": "^2.0.20",
		"@types/errorhandler": "0.0.30",
		"@types/express": "^4.11.1",
		"@types/express-session": "0.0.32",
		"@types/jwt-simple": "^0.5.33",
		"@types/lodash": "^4.14.63",
		"@types/md5": "^2.1.32",
		"@types/multer": "^1.3.6",
		"@types/node": "^14.18.20",
		"@types/pg": "^7.14.11",
		"@types/qs": "^6.5.1",
		"@types/socket.io": "^1.4.38",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.2",
		"babel-plugin-istanbul": "^4.1.5",
		"babel-plugin-root-import": "^5.1.0",
		"babel-plugin-transform-async-functions": "^6.22.0",
		"babel-plugin-transform-builtin-extend": "^1.1.2",
		"babel-plugin-transform-regenerator": "^6.26.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-register": "^6.26.0",
		"babel-watch": "^2.0.7",
		"concurrently": "^3.4.0",
		"eslint": "^8.38.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.2.1",
		"node-sass": "^6.0.1",
		"nodemon": "^1.11.0",
		"pg": "^8.9.0",
		"prettier": "2.8.2",
		"sequelize-cli": "^6.6.0",
		"ts-loader": "^2.3.7",
		"ts-node": "^3.3.0",
		"tslint": "^5.0.0",
		"webpack": "^3.6.0"
	},
	"engines": {
		"node": ">=16.20.2"
	}
}
